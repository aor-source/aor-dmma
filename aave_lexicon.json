  "grammatical_patterns": {
    "habitual_be": {
      "description": "Habitual aspect marker",
      "examples": ["I be working", "She be knowing", "They be playing"],
      "regex": "\\b(I|you|he|she|it|we|they)\\s+be\\s+\\w+ing\\b"
    },
    "bin_perfect": {
      "description": "Remote past/stressed BIN",
      "examples": ["I been knowing", "She been had that"],
      "regex": "\\b(been|BIN)\\s+(\\w+ing|had|done)\\b"
    },
    "done_perfective": {
      "description": "Completive aspect",
      "examples": ["done finished", "done told you"],
      "regex": "\\bdone\\s+\\w+ed\\b"
    },
    "negative_concord": {
      "description": "Multiple negation",
      "examples": ["ain't no", "don't got no", "can't never"],
      "regex": "\\b(ain't|don't|can't|won't|didn't)\\s+(no|nothing|nobody|never)\\b"
    },
    "copula_deletion": {
      "description": "Zero copula",
      "examples": ["she nice", "he cool", "they ready"],
      "regex": "\\b(he|she|it|they|we|you)\\s+(nice|cool|ready|fine|good|bad|real|wild)\\b"
    },
    "invariant_be": {
      "description": "Uninflected be",
      "examples": ["he be", "she be", "it be"],
      "regex": "\\b(he|she|it)\\s+be\\b"
    }
  },
